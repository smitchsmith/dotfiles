<!DOCTYPE html>
<html>
<head>
  <title>Friends</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= debug params %>

<%= flash[:errors].inspect if flash[:errors]%>

<% if current_user %>
  <%= "Welcome, #{current_user.email}" %>
  <%= button_to "Log Out", session_url, method: "delete" %>
<% else %>
  <p> <%= link_to "Log In", new_session_url %> -
  <%= link_to "Sign Up", new_user_url %> </p>
<% end %>

<%= yield %>

</body>
</html>