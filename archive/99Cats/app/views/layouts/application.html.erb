<!DOCTYPE html>
<html>
<head>
  <title>Cats</title>
  <%#= stylesheet_link_tag    "application", :media => "all" %>
  <%#= javascript_include_tag "application" %>
  <%#= csrf_meta_tags %>
</head>
<body>
<%= debug params %>

<% if flash[:errors] %>
   <%= flash[:errors] %>
   <br><br>
<% end %>

<% if current_user.nil? %>
  <%= link_to "Log In", new_session_url %>
<% else %>
  <%= link_to "Hello, #{current_user.user_name}", user_url(current_user)%>
  <br><br>
  <%= button_to "Log Out", delete_session_url, method: "delete"%>
<% end %>
<%= yield %>

</body>
</html>